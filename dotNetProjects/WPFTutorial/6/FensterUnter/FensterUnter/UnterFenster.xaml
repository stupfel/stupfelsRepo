<Window x:Class="FensterUnter.UnterFenster"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="UnterFenster" Height="300" Width="300"
        WindowStartupLocation="CenterOwner">
    <StackPanel>
        <Button x:Name="btnOk" Click="btnOk_Click" Margin="5">OK</Button>
        <Button x:Name="btnAbbrechen" Click="btnAbbrechen_Click" Margin="5">Abbrechen</Button>
        <TextBox x:Name="txtBox" Margin="5"></TextBox>
    </StackPanel>
</Window>
